{
    "name": "carloshaam/d4sign-sdk",
    "description": "A PHP library to interact with the D4Sign API",
    "keywords": ["d4sign", "sdk"],
    "type": "library",
    "homepage": "https://github.com/carloshaam/d4sign-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "carloshaam",
            "email": "carloshaam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "D4Sign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D4Sign\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
